{% extends('base.html.twig') %}

{% block title %}  Search By Weather Results | AdeoWeb What To Wear Today &trade; - Coding Challenge by Amr Farouq {% endblock %}

{% block body %}
    <!-- Main -->
    <section id="main" class="wrapper style1">
        <header class="major">
            <h2>AdeoWeb - What To Wear Today? &trade; | Search By Weather Results</h2>
            <h3><span><img src="{{ asset('init/images/logo.png') }}" alt="logo" width="250px;"/></span></h3>
        </header>
    </section>



    <!-- Three -->
    <section id="one" class="wrapper style1">
        <div class="container">
            <header class="major">
                <h3>Results By Custom Weather</h3>

                <ul id="myUL">
                    {% for product in products %}

                        <li><a href="#">{{ product.productName }}</a></li>

                    {% endfor %}
                </ul>
            </header>
        </div>
    </section>

{% endblock %}